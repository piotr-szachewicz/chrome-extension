{
  "manifest_version": 2,

  "name": "Hello world Google Extension",
  "description": "This extension doesn't do anything",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
